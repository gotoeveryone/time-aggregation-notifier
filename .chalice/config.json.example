{
  "version": "2.0",
  "app_name": "time-aggregation-notifier",
  "stages": {
    "dev": {
      "autogen_policy": true
    },
    "prod": {
      "manage_iam_role": false,
      "iam_role_arn": "arn:aws:iam::XXXXXXXX:role/XXX",
      "environment_variables": {
        "TZ": "Asia/Tokyo",
        "REDMINE_CUSTOM_FIELD_NAME": "client",
        "REDMINE_PROJECT": "project"
      }
    }
  }
}
